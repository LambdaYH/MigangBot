datastore_data_dir = data/datastore/data
datastore_database_url = postgresql+asyncpg://migangbot_user:1206@db:5432/migangbot_db
SQLALCHEMY_DATABASE_URL = postgresql+asyncpg://migangbot_user:1206@db:5432/migangbot_db

# chatrecord
chatrecorder_record_send_msg = false # 不记录自身消息，防止转换获得的base64被缓存并替换消息段后无法正常发出
